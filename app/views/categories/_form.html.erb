<%= form_with(model:category,method: "post") do |f|%>
  <%= f.label "Kategori ekle",class:"block font-bold text-4xl mb-7"%>
  <%= f.text_field :title,class:"g-green-50 border border-green-500 rounded"%>
  <%= f.submit "Ekle",class:"border border-red-300 block font-bold text-2xl rounded bg-red-300 text-black-500 p-3 mt-3"%>
<% end%>
